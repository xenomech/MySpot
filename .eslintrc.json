{
  "env": {
    "browser": true,
    "es6": true
  },
  "extends": ["plugin:react/recommended", "google", "prettier"],
  "parserOptions": {
    "ecmaVersion": 2018,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "plugins": ["react"],
  "rules": {
    "require-jsdoc": 0,
    "react/react-in-jsx-scope": "off",
    "react/prop-types": 0
  },
  "settings": {
    "react": {
      "version": "latest"
    }
  }
}
